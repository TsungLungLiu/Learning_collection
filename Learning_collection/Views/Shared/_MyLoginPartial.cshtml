

@{ 
    var user = Session["user"] as string;
}





@if (Session["user"] == null)
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Register", "Register", "MyAccount")</li>
        <li>@Html.ActionLink("Log in", "Login", "MyAccount")</li>
    </ul>
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink(user, "Register", "MyAccount")</li>
        <li>@Html.ActionLink("Log out", "Logout", "MyAccount")</li>
    </ul>
}









