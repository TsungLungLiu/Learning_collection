@model Learning_collection.Models.Package
@{
    
}

<h2>ShowData_1</h2>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Forget_ID</title>
</head>
<body>
    <table>
        <tr style="border:1px solid green; margin: 15px;">
            <td>Id</td>
            <td>Firstname</td>
            <td>Lastname</td>
            <td>Email</td>
        </tr>
        @for (int i = 0; i < Model.Email.Count; i++)
        {
            <tr style="border:1px solid green; margin: 15px;">
                <td>@Model.Id[i]</td>
                <td>@Model.Firstname[i]</td>
                <td>@Model.Lastname[i]</td>
                <td>@Model.Email[i]</td>
            </tr>
        }

    </table>
</body>
</html>